<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="301.0" prefWidth="413.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <Label layoutX="20.0" layoutY="14.0" text="Calculate Sum" />
      <TextField fx:id="txtNumber1" layoutX="21.0" layoutY="31.0" promptText="Firts number" />
      <TextField fx:id="txtNumber2" layoutX="21.0" layoutY="66.0" promptText="Second number" />
      <Button fx:id="btSum" layoutX="20.0" layoutY="138.0" mnemonicParsing="false" onAction="#onBtSumAction" text="Sum" />
      <Label layoutX="21.0" layoutY="244.0" text="Result = " />
      <Label fx:id="labelResult" layoutX="95.0" layoutY="244.0" />
      <ComboBox fx:id="comboboxPerson" layoutX="207.0" layoutY="66.0" onAction="#onComboboxPersonAction" prefWidth="150.0" promptText="Select a person" />
      <Button fx:id="btAll" layoutX="327.0" layoutY="101.0" mnemonicParsing="false" onAction="#onBtAllAction" text="All" />
   </children>
</AnchorPane>
